		<div id="settings">
			<ul class="settings">
				<li>Interval:</li>
				<li class="period"><a href="#" interval="1440">1d</a></li>
				<li class="period"><a href="#" interval="720">12h</a></li>
				<li class="period"><a href="#" interval="180">3h</a></li>
				<li class="period"><a href="#" interval="60">1h</a></li>
				<li class="period"><a href="#" interval="30">30m</a></li>
				<li class="period current"><a href="#" interval="15">15m</a></li>
				<li class="period"><a href="#" interval="5">5m</a></li>
				<li class="period"><a href="#" interval="1">1m</a></li>

				<li class="select">
					<ul>
						<li class="chart_type current"><a href="#" type="candlestick">Candlestick</a></li>
						<li class="chart_type"><a href="#" type="ohlc">OHLC</a></li>
						<li class="chart_type"><a href="#" type="spline">Line</a></li>
					</ul>
				</li>

				<li class="indicator_ma">
					MA
					<ul class="first">
						<li class="indicator_interval_first" bars="0">None</li>
						<li class="indicator_interval_first" bars="5">5 bars</li>
						<li class="indicator_interval_first" bars="10">10 bars</li>
						<li class="indicator_interval_first" bars="15">15 bars</li>
						<li class="indicator_interval_first" bars="20">20 bars</li>
						<li class="indicator_interval_first" bars="30">30 bars</li>
					</ul>
					<ul class="second">
						<li class="indicator_interval_second" bars="0">None</li>
						<li class="indicator_interval_second" bars="10">10 bars</li>
						<li class="indicator_interval_second" bars="20">20 bars</li>
						<li class="indicator_interval_second" bars="30">30 bars</li>
						<li class="indicator_interval_second" bars="40">40 bars</li>
						<li class="indicator_interval_second" bars="50">50 bars</li>
					</ul>
				</li>
			</ul>
